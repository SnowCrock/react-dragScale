{
  "name": "react-dragzoom",
  "version": "0.3.0",
  "description": "a drag and scale component for react",
  "main": "index.js",
  "author": "lizhangrui",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env.dev --inline --progress --colors",
    "test": "jest",
    "clean": "rm -rf dist",
    "build": " npm run build-es && npm run build-lib ",
    "build-dev": "rm -rf dist && webpack --env.dev --progress --profile --colors",
    "build-lib": "rm -rf lib && webpack --env.prod --progress --profile --colors",
    "build-es": " rm -rf es && babel src --out-dir es && lessc src/index.less > es/index.css ",
    "build-prod": "rm -rf dist && webpack --env.prod --progress --profile --colors",
    "lint": "eslint \"src/**/*.@(js|jsx)\" -f node_modules/eslint-html-reporter/reporter.js -o eslint_report.html",
    "flow": "flow check || exit 0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-draggable": "^2.2.6"
  },
  "devDependencies": {
    "react-test-renderer": "^15.6.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.4.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.1",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.48.0",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "open-browser-webpack-plugin": "0.0.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.15.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}

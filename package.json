{
  "name": "react-dragzoom",
  "version": "0.2.0",
  "description": "a drag and scale component for react",
  "main": "index.js",
  "author": "lizhangrui",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env.dev --inline --progress --colors",
    "test": "jest",
    "clean": "rm -rf dist",
    "build": " npm run build-es",
    "build-es": " rm -rf es && babel src --out-dir es && lessc src/index.less > es/index.css ",
    "lint": "eslint \"src/**/*.@(js|jsx)\" -f",
    "flow": "flow check || exit 0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-draggable": "^2.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.48.0",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.13.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
